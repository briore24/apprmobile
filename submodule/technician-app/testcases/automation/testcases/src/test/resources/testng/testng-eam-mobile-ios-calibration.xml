<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CalibrationApp_EAM_iOS_Test Suite">
	<parameter name="configPath"  value="src/test/resources/config.eam.mobile.ios.calibration.properties"/>
	<listeners>
		<listener class-name="com.ibm.maximo.technician.listener.ScreenShotListener" />
    	<listener class-name="com.ibm.maximo.technician.listener.FastFailClass"/>
  	</listeners>
	<parameter name="testType"  value="mobileapp"/>
	<test thread-count="1" name="Datasheet Tests">
		<groups>
			<run>
				<include name="mobile" />
			</run>
		</groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.calibration.tests.SUMIWOAnalogDatasheetTests" />
		</classes>
	</test>
</suite> 