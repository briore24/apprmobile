<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MobileTestSuite MAS/FVT priority=1 test run on iOS device">
	<parameter name="configPath" value="src/test/resources/config.mas.mobile.ios.properties" />
	<parameter name="testType" value="mobileapp" />
	<listeners>
		<listener class-name="com.ibm.maximo.technician.listener.ScreenShotListener" />
	</listeners>
	<test thread-count="1" name="CombineTest" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.CombineTest" />
		</classes>
	</test>
	<test thread-count="1" name="FailureReportTest" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.FailureReportTest" />
		</classes>
	</test>
	<test thread-count="1" name="TaskCardTest" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.TaskCardTest" />
		</classes>
	</test>
	<test thread-count="1" name="CompleteWorkOrder" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.CompleteWorkOrder" />
		</classes>
	</test>
	<test thread-count="1" name="PlannedToolsTest" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.PlannedToolsTest" />
		</classes>
	</test>
	<test thread-count="1" name="AutomaticallyStopWorkOrderAndStartWorkOrderTest" preserve-order="true">
		<groups><run><include name="mobile" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.AutomaticallyStopWorkOrderAndStartWorkOrderTest" />
		</classes>
	</test>
</suite>