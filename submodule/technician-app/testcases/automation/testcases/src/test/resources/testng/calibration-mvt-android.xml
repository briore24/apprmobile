<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CalibrationApp_FVT_Android_Test Suite">
	<parameter name="configPath" value="src/test/resources/config.mas.mobile.android.calibration.properties"/>
	<listeners>
		<listener class-name="com.ibm.maximo.technician.listener.ScreenShotListener" />
	</listeners>
	<parameter name="testType" value="mobileapp"/>
	<test thread-count="1" name="Datasheet Tests">
		<groups>
			<run>
				<include name="mobile" />
			</run>
		</groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.calibration.tests.SUMIWOAnalogDatasheetTests" />
		</classes>
	</test>
	<test thread-count="1" name="Calibration History Test">
		<groups>
			<run>
				<include name="mobile" />
			</run>
		</groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.calibration.tests.CalibrationHistoryTest" />
		</classes>
	</test>
	<test thread-count="1" name="Datasheet Standard Deviation Tests">
		<groups>
			<run>
				<include name="mobile" />
			</run>
		</groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.calibration.tests.EUIRPTSTDDEVAnalogDatasheetTests" />
		</classes>
	</test>
</suite>