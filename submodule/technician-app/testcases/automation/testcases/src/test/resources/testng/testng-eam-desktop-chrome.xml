			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.ReportLaborPremiumHours" />
		</classes>
	</test>
	<test thread-count="1" name="CombineTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.CombineTest" />
		</classes>
	</test>
	<test thread-count="1" name="TaskCardTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.TaskCardTest" />
