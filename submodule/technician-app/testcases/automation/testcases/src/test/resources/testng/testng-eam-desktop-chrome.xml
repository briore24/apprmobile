		</classes>
	</test>
	<test thread-count="1" name="AutomaticallyStopWorkOrderAndStartWorkOrderTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.AutomaticallyStopWorkOrderAndStartWorkOrderTest" />
		</classes>
	</test>
</suite>