<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DesktopTestSuite">
	<parameter name="configPath"  value="src/test/resources/config.eam.desktop.chrome.properties"/>
	<parameter name="testType"  value="desktop"/>
	<listeners>
		<listener class-name="com.ibm.maximo.technician.listener.ScreenShotListener" />
	</listeners>
	<test thread-count="1" name="MultiAssetLocationTouchpointsTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.MultiAssetLocationTouchpointsTest" />
		</classes>
	</test>
	<test thread-count="1" name="AddDeleteMultiAssetLocationInFollowUpWorkTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.AddDeleteMultiAssetLocationInFollowUpWorkTest" />
		</classes>
	</test>
	<test thread-count="1" name="ViewAndEditSpecificationsTest" preserve-order="true">
		<groups><run><include name="desktop" /></run></groups>
		<classes>
			<class name="com.ibm.maximo.technician.testcases.TestSuite" />
			<class name="com.ibm.maximo.technician.testcases.ViewAndEditSpecificationsTest" />
		</classes>
	</test>
	<test thread-count="1" name="ReportLaborPremiumHours" preserve-order="true">
