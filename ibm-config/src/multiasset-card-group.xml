<card-group datasource="{page.state.multiAssetData}" can-sort="true" enable-barcode-scanner="true" readers="{[`ean_reader`]}" timeout="{45}" hide-count-search="{(page.state.selectedSwitch===1)}" no-select-behavior="true" id="kj5n5">
