  <multi-sort-button slot="actions" kind="ghost" datasource="{page.state.multiAssetData}" icon="Carbon:caret--sort" id="mryw3"/>
  <common-card-template slot="item" chevron-kind="ghost" show-chevron="{item.asset}" on-chevron-click="navigateMultiAssetToAssetDetails" on-chevron-click-arg="{item}" id="cardtemplate2">
    <border-layout slot="slot1" fill-parent="true" id="bm_6v">
      <start id="b6rp7" horizontal-overflow="visible" width="5">
        <box vertical-align="center" direction="row" horizontal-align="start" fill-parent="true" id="gqyn6">
