      <wrapped-text slot="slot1" size="large" label="{item.computedWorkTitle}" sub-label="{item.worktype}" id="n942w"/>
      <tag-group slot="slot2" align="end" tags="{item.computedWOStatusPriority}" id="vadn6"/>
      <box slot="slot3" manage-children="false" id="mbj24">
        <icon fill="red" icon="carbon:warning--alt--filled" hidden="{item.actfinish || (!item.computedIsOverDue || (!item.targcompdate &amp;&amp; !item.schdate))}" id="b38em"/>
        <date-relative value="{item.targcompdate || item.schedfinish}" type="date_full_month" text-prefix="Due" hidden="{item.actfinish || (item.computedIsOverDue || !item.targcompdate)}" id="qj_43"/>
        <date-relative value="{item.targcompdate || item.schedfinish}" type="date_full_month" text-prefix="Overdue since" theme="error" hidden="{item.actfinish || (!item.computedIsOverDue || !item.targcompdate)}" id="qj_467"/>
        <date-relative value="{item.actfinish}" type="date_full_month" text-prefix="Work completed" theme="default" hidden="{!(item.status === 'COMP' ||  item.status === 'CAN') &amp;&amp; (!item.actfinish)}" id="qj_577"/>
      </box>
      <box slot="slot4" direction="column" fill-parent="true"  children-sizes="50,50" id="jn55d">
        <box direction="row" padding-start=".8"  padding-top=".5" padding-bottom=".5" hidden="{!item.assetnumber}" children-sizes="5,95" id="jn05d">
          <box id="aan8m" fill-parent="true" padding="0">
            <icon icon="carbon:asset" id="w5a20"/>
          </box>
          <box id="k8tbm" fill-parent="true" direction="column">
            <label label="{item.assetnumber}" padding="none" id="pki6r"/>
            <wrapped-text label="{item.assetdesc}" max-length="250" padding="none"  size="medium" id="n2z6k"/>
          </box>
        </box>
        <box direction="row" padding-start=".8"  fill-parent="true" padding-top=".5" padding-bottom=".5" children-sizes="5,95" hidden="{!item.locationnum}" id="jn53d">
          <box fill-parent="true" id="aan8k">
            <icon icon="carbon:location" id="w5a25"/>
          </box>
          <box fill-parent="true" id="k8gbm" direction="column">
            <label label="{item.locationnum}" padding="none" id="pyp6r"/>
            <wrapped-text size="medium" padding="none" max-length="250" label="{item.locationdesc}" id="jybg7"/>
          </box>
        </box>
      </box>
      <box slot="slot5" direction="column" fill-parent="true"  children-sizes="50,50" id="ja55d">
        <box direction="row" padding-start=".8"  fill-parent="true" padding-top=".5" padding-bottom=".5" children-sizes="5,95" hidden="{!(item.formattedaddress || item.serviceaddressdesc)}" id="jna3d">
          <box fill-parent="true" id="aaa8k">
            <icon icon="carbon:map" id="w5a2a"/>
          </box>
          <field wrap="true" values-top-padding="false" label="Service Address" padding="bottom" value="{item.formattedaddress ? item.formattedaddress : item.serviceaddressdesc}" id="ykjv9"/>
        </box>
        <box direction="row" padding-top=".5" padding-bottom=".5"  children-sizes="5,95" id="jna5d">
          <box id="aanam" fill-parent="true" padding="0">
            <button  kind="secondary" icon="maximo:cost" on-click="openWoTotalCostDrawer" on-click-arg="{{'item': item, 'datasource': wodetails}}" id="p838v"/>
          </box>
          <box id="a8tbm" fill-parent="true" direction="column">
            <field  swap-position="true" wrap="true" qualifier="{`${app.client.userInfo.baseCurrency}`}" label="Cost" values-top-padding="false" padding="bottom" value="{item.accessWoCostData}" id="q2dq2"/>
          </box>
        </box>
      </box>
      <button-group padding="false" slot="slot6" id="bxek9" hidden="{app.state.hideFeature}">
        <button kind="secondary" icon="maximo:materialsandtools" on-click="openDrawer" on-click-arg="{{'item': item, 'datasource': wodetails}}" hidden="{item.computedDisableButton}" id="br_wn"/>
        <button kind="secondary" icon="carbon:map" disconnected="{!app.state.isMapValid}" disconnected-message="Map is not configured" hidden="{!item.autolocate}" on-click="openMapPage" on-click-arg="{{'item': item, 'prevPage': 'schedulecardlist'}}" id="jya7d"/>
        <button kind="secondary" icon="carbon:document" on-click="openWorkLogDrawer" on-click-arg="{{'item': item}}" id="eyw9b"/>
        <button kind="secondary" icon="carbon:operation--gauge" on-click="openMeterReadingDrawer" hidden="{(!item.assetmetercount &amp;&amp; !item.locationmetercount)}" disabled="{!app.checkSigOption(`${app.state.woOSName}.ENTERREADINGS`)}" on-click-arg="{{'item': item}}" id="vqxv2"/>
        <button kind="secondary" icon="carbon:study--previous" loading="{item.wonum === page.state.currentItem &amp;&amp; app.state.assignmentLoading}" on-click="openReassignmentDialog" on-click-arg="{{'item': item, 'datasource': wodetails}}" tag-type="dark-gray" hidden="{['COMP', 'CAN', 'CLOSE'].includes(item.status_maxvalue) || !app.checkSigOption(`${app.state.woOSName}.ALLOWREASSIGNMENT`)}" id="reassignment_list"/>
      </button-group>
      <button-group slot="slot7" id="kvd2r" padding="false" align="end" hidden="{item.computedWorkTypeStatus || app.state.hideFeature}">
