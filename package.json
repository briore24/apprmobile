{
  "name": "@maximo/apprmobile",
  "author": "Maximo Dev <maximodev@us.ibm.com>",
  "version": "9.1.0",
  "private": true,
  "homepage": ".",
  "windowsInstall": true,
  "winShell": false,
  "dependencies": {
    "@maximo/maximo-js-api": "3.1.517",
    "@maximo/react-components": "3.1.517",
    "@maximo/maximo-library": "3.1.517",
    "mobx": "5.15.7",
    "mobx-react": "5.4.4",
    "react": "17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "cd build/app && set NODE_OPTIONS=\"--max_old_space_size=30000\" && react-scripts start",
    "build": "npx --no-install maxdev-cli build --in ./src/app.xml --out ./build/app/src/ --update-ids --copy",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./src/",
    "build:production": "yarn build && yarn labels && cd build/app && react-scripts build",
    "ci:check": "npm run test:coverage && npm run lint && npm run build",
    "man": "npx --no-install maxdev-cli man",
    "test": "react-scripts test --watchAll --coverage",
    "test:coverage": "cross-env CI=true npx react-scripts test --coverage --silent --all --passWithNoTests",
    "clean": "npx --no-install maxdev-cli clean",
    "labels": "npx --no-install maxdev-cli localize -i ./src/app.xml -o ./src/i18n/labels.json --format json",
    "build:maximoMobile:all:dev": "yarn build:production && npx maxdev-cli upload-app --mobile-only",
    "deploy": "npx cross-env NODE_OPTIONS=\"--max_old_space_size=30000\" GRAPHITE_SOURCEMAP=inline-source-map yarn build:production && npx maxdev-cli upload-app -u maxadmin -p maxadmin --port 7001"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@maximo/app-processor": "3.1.517",
    "@maximo/maxdev-cli": "3.1.517",
    "@maximo/graphite-cli": "3.1.517",
    "cross-env": "^7.0.2",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "react-test-renderer": "17.0.2",
    "sinon": "7.5.0"
  },
  "jest": {
    "coverageThreshold": {
      "./src/**/*.js": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "src/SharedResources/maximo/",
      "src/scripts/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(ol|ol-mapbox-style|geotiff|quick-lru))"
    ]
  },
  "graphite": {
    "platform": "maximoMobile",
    "sharedResources": {
      "maximo": {
        "library": "@maximo/maximo-library"
      }
    }
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "parse-path": "5.0.0",
    "parse-url": "8.1.0",
    "postcss": "8.4.38",
    "semver": "7.6.0",
    "tough-cookie": "4.1.3",
    "@carbon/telemetry": "0.1.0",
    "@babel/traverse": "7.23.2"
  }
}